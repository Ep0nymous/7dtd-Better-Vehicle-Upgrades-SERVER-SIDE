<?xml version="1.0" encoding="UTF-8"?>

<ePONYmous>

<append xpath="/items">

<!-- Adding a new Turbo Charger schematic -->
	<item name="modVehicleTurboChargerSchematic">
		<property name="Extends" value="schematicNoQualityMaster"/>
		<property name="CreativeMode" value="Player"/>
		<property name="CustomIcon" value="modVehicleSuperCharger"/>
		<property name="CustomIconTint" value="128,128,255"/>
		<property name="Unlocks" value="modVehicleTurboCharger"/>
		<effect_group tiered="false">
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="ModifyCVar" cvar="modVehicleTurboCharger" operation="set" value="1"/>
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="GiveExp" exp="60"/>
		</effect_group>
	</item>

